{
  "name": "uemcp-server",
  "version": "0.9.0",
  "description": "Unreal Engine Model Context Protocol Server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "version:bump": "node ../scripts/bump-version.js",
    "version:major": "node ../scripts/bump-version.js --type=major",
    "version:minor": "node ../scripts/bump-version.js --type=minor",
    "version:patch": "node ../scripts/bump-version.js --type=patch",
    "version:beta": "node ../scripts/bump-version.js --prerelease=beta",
    "version:alpha": "node ../scripts/bump-version.js --prerelease=alpha"
  },
  "keywords": [
    "mcp",
    "unreal-engine",
    "ai-assistant"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
